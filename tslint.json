{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "*.json", "**/*.json"]
  },
  "rules": {
    "no-string-literal": false,
    "object-literal-sort-keys": [false],
    "jsx-no-lambda": [false],
    "no-console": [false]
  }
}
